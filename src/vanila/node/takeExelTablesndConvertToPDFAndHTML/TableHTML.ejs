<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Table</title>
</head>

<body>
    <table>
        <thead>
            <tr>
                <% header.forEach(column =>{ %>

                <th>
                    <%- column %>
                </th>

                <%   } ) %>
            </tr>

        </thead>
        <tbody>
            <% body.forEach(rows =>{ %>

            <tr>
                <% rows.forEach(row =>{ %>
                <th>
                    <%- row %>
                </th>
                <% }) %>

            </tr>

            <%   } ) %>
        </tbody>
    </table>
</body>

</html>